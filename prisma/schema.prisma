generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cofe {
  id      Int @id @default(autoincrement())
  nome    String
  tipo    String
  preco   Float
  des     String
  tags    TagCafe[]
  //itenas  ItemPedido[]
}

model TagCafe{
  id      Int   @id @default(autoincrement())
  nome    String
  cofeId  Int
  cofe    Cofe  @relation(fields: [cofeId], references: [id])
}


